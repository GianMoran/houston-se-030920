<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Gender</th>
  </tr>
<% @patients.each do | patient | %>
    <tr>
        <td><a href="/patient/<%= patient.id %>"><%= patient.name %></a></td>
        <td><%= patient.age %></td>
        <td><%= patient.gender %></td>
    </tr>    
<% end %>
</table>